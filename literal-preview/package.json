{
    "name": "literal-preview",
    "displayName": "Literal Preview",
    "description": "Preview literals with different numerical representations.",
    "version": "1.0.1",
    "publisher": "dbaumeis",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.enablePreviewLiterals",
        "onCommand:extension.disablePreviewLiterals"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.enablePreviewLiterals",
                "title": "Enable: Preview Literals"
            },
            {
                "command": "extension.disablePreviewLiterals",
                "title": "Disable: Preview Literals"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
